<div class="flex-container" id="emp_awards" hidden>
	<table>
		<thead>
			<tr>
				<th>Granted By</th>
				<th>Award Title</th>
				<th>Award Recipient</th>
				<th>Grant Date</th>
			</tr>
		</thead>

		<tbody>
			<% results.forEach(function(result){ %>
			<tr>
				<th><%=result.granted_by%></th>
				<td><%=result.title%></td>
				<td><%=result.award_recipient%></td>
				<td><%=result.grant_date %></td>
			</tr>
			<% }); %>
		</tbody>
	</table>
</div>
