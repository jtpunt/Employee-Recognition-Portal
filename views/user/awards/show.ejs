<% include ../../partials/header %>

<h1>This is awards show page11</h1>

<table>
	<caption>Who wins? Cell content indicates the status of the row entry</caption>
	<thead>
		<tr>
			<th>Granted By</th>
			<th>Award Title</th>
			<th>Award Recipient</th>
			<th>Grant Date</th>
		</tr>
	</thead>
	<tfoot>
		<tr>
			<td colspan="2">Footer 1</td>
			<td colspan="4">Footer 2</td>
		</tr>
	</tfoot>
	<tbody>
		<% results.forEach(function(result){ %>
		<tr>
			<th><%=result.granted_by%></th>
			<td><%=result.title%></td>
			<td><%=result.award_recipient%></td>
			<td><%=result.grant_date %></td>
		</tr>
		<% }); %>
	</tbody>
</table>

<% include ../../partials/footer %>