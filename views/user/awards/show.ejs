<% include ../../partials/header %>

<h1>This is awards show page1</h1>

<table>
	<caption>All awards</caption>
	<thead>
		<tr>
			<th>Granted By</th>
			<th>Award Title</th>
			<th>Award Recipient</th>
			<th>Grant Date</th>
		</tr>
	</thead>
	
	<tbody>
		<% results.forEach(function(result){ %>
		<tr>
			<th><%=result.granted_by%></th>
			<td><%=result.title%></td>
			<td><%=result.award_recipient%></td>
			<td><%=result.grant_date %></td>
		</tr>
		<% }); %>
	</tbody>
</table>


<a href="/user">Back to User page</a>

<% include ../../partials/footer %>